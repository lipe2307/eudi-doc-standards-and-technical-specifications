{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "transaction_id": {
            "type": "string",
            "maxLength": 36,
            "minLength": 1,
            "examples": [
                "8D8AC610-566D-4EF0-9C22-186B2A5ED793"
            ]
        },
        "date_time": {
            "type": "string",
            "format": "date-time",
            "examples": [
                "2025-11-13T20:20:39+00:00"
            ]
        },
        "service": {
            "type": "string",
            "maxLength": 50,
            "examples": [
                "Superbank Onlinebanking"
            ]
        },
        "action": {
            "type": "string",
            "maxLength": 140,
            "examples": [
                "Login to your online account."
            ]
        }
    },
    "additionalProperties": true,
    "required": [
        "date_time",
        "service",
        "action"
    ]
}
